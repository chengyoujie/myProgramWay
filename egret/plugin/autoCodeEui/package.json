{
	"name": "autocodeeui",
	"description": "exml生成对应的代码模块【传奇来了项目组】",
	"version": "1.0.0",
	"publisher": "cyj",
	"categories": [
		"Other"
	],
	"icon": "images/icon.svg",
	"bugs": {
		"url": "https://github.com/egret-labs/wing-extensions/issues"
	},
	"homepage": "https://github.com/egret-labs/wing-extensions/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/egret-labs/wing-extensions.git"
	},
	"license": "MIT",
	"activationEvents": [
		"onCommand:extension.autocodeeui",
		"onCommand:extension.autocodeeuiconfig",
		"onCommand:extension.autocodeeuimodule"
	],
	"engines": {
		"wing": "^3.0.4"
	},
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.autocodeeui",
				"title": "AutoCodeEUI"
			},
			{
				"command": "extension.autocodeeuiconfig",
				"title": "AutoCodeEUIConfig"
			},
			{
				"command": "extension.autocodeeuimodule",
				"title": "AutoCodeEUIModule"
			}
		],
		"keybindings": [{
			"command": "extension.autocodeeui",
			"key": "f12",
			"mac": "f12"
    	}]
	},
	"scripts": {
		"wing:prepublish": "node ./node_modules/egretwing/bin/compile",
		"compile": "node ./node_modules/egretwing/bin/compile -watch -p ./",
		"postinstall": "node ./node_modules/egretwing/bin/install"
	},
	"dependencies": {},
	"devDependencies": {
		"egretwing": "^3.0.4",
		"typescript": "^1.7.5"
	}
}
