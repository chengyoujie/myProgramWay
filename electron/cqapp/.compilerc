{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "4.0"
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator",
          "transform-remove-strict-mode"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "4.0"
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator",
          "transform-remove-strict-mode"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
